{
  "name": "MegaLinks & Snahp.it Forum Link Solver",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Automatically find and decode all /r/megalinks and forum.snahp.it post links.",
  "author": "Sam Kauffman (SamVK)",
  "icons": {
      "16": "img/icon/icon-16.png",
      "48": "img/icon/icon-48.png"
  },
  "content_scripts": [{
        "matches": [
            "https://forum.snahp.it/viewtopic.php*",
            "https://*.reddit.com/r/megalinks/comments/*"
        ],
        "js": [ "forum/index.js" ],
        "css": [ "forum/style.css" ]
    }, {
        "matches": [ "*://links.snahp.it/*" ],
        "js": [ "links/index.js" ],
        "css": [ "links/style.css" ]
    }]
}
